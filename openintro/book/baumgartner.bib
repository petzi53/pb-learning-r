@article{Shum_1997,
	Author = {Shum, Simon Buckingham},
	Date-Added = {2011-12-29 23:06:17 +0100},
	Date-Modified = {2011-12-29 23:06:18 +0100},
	Doi = {10.3217/jucs-003-08-0899},
	File = {S. Buckingham Shum\: Negotiating the Construction and Reconstruction of Organisational Memories:/Users/peter/Library/Application Support/Firefox/Profiles/me851qy9.default/zotero/storage/13211/Buckingham_Shum_S.pdf:PDF},
	Journal = j-jucs,
	Number = {8},
	Owner = {peter},
	Pages = {899--928},
	Timestamp = {24.11.2008},
	Title = {Negotiating the Construction and Reconstruction of Organisational Memories},
	Url = {http://www.jucs.org/jucs_3_8/negotiating_the_construction_and/Buckingham_Shum_S.html},
	Urldate = {2008-11-24},
	Volume = {3},
	Year = {1997},
	Abstract = {This paper describes an approach to capturing organisational memory,
	which serves to ground an analysis of human issues that knowledge
	management (KM) technologies raise. In the approach presented, teams
	construct graphical webs of the arguments and documents relating
	to key issues they are facing. This supports collaborative processes
	which are central to knowledge work, and provides a group memory
	of this intellectual investment. This approach emphasises the centrality
	of negotiation in making interdisciplinary decisions in a changing
	environment. Discussion in the paper focuses on key human dimensions
	to KM technologies, including the cognitive and group dynamics set
	up by an approach, the general problem of preserving contextual cues,
	and the political dimensions to formalising knowledge processes and
	products. These analyses strongly motivate the adoption of participatory
	design processes for KM systems.},
	Bdsk-Url-1 = {http://www.jucs.org/jucs_3_8/negotiating_the_construction_and/Buckingham_Shum_S.html},
	Bdsk-Url-2 = {http://dx.doi.org/10.3217/jucs-003-08-0899}}

