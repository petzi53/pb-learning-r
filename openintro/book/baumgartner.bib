@techreport{Baumgartner:2011a,
	Author = {Baumgartner, Peter},
	Date-Added = {2012-12-08 11:48:25 +0000},
	Date-Modified = {2012-12-08 11:51:19 +0000},
	Institution = {Webpublikation},
	Keywords = {pb-pub, taxonomy, pattern},
	Local-Url = {file://localhost/Users/peter/Documents/Papers/pdv-e-book-kapitel-2.pdf},
	Title = {Taxonomie von Lernzielen - ein Fallbeispiel: Kapitel 2 von "Taxonomie von Unterrichtsmethoden -- Ein Pl{\"a}doyer f{\"u}r didaktische Vielfalt},
	Url = {http://www.peter.baumgartner.name/material/article/PDV-e-book-Kapitel-2.pdf},
	Urldate = {2012-12-08},
	Year = {2011},
	Abstract = {Das Buch "A Taxonomy for Learning, Teaching and Assessing -- A Revision of Bloom's Taxonomy of Educational Objectives"  baut -- wie aus dem Titel bereits ersichtlich -- auf die 1956 von Benjamin Bloom herausgegebene Taxonomie auf. Die Bloom'sche Taxonomie ist heute bereits durchaus als Klassiker anzusehen, d. h. Bem{\"u}hungen zur systematischen Gliederung von Lern- und Lehrzielen m{\"u}ssen sie als Grundlage bzw. Benchmark referenzieren. Ihr Grundmuster ist eine Liste von sechs kognitiven Bildungszielen, wovon jedes wiederum unterschiedlich detailliert unterteilt ist. Die Taxonomie ist damit eindimensional aufgebaut. Darin besteht ein wesentlicher Unterschied zu ihrer zweidimensionalen Erweiterung in Form einer Tabelle durch Anderson und KollegInnen.

Das im Kapitel 2 vorgestellte Beispiel einer Taxonomie soll erste Anregungen vermitteln, wie Klassifikationssysteme aufgebaut sind, welche Funktionen sie wahrnehmen und wie sie benutzt werden k{\"o}nnen. Gleichzeitig ist es aber eine Gebrauchsanweisung, die f{\"u}r die Gestaltung von Lernzielen bzw. Lernergebnisse ("Learning Outcomes") bei der curricularen Entwicklung genutzt werden kann.},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.peter.baumgartner.name/material/article/PDV-e-book-Kapitel-2.pdf}}

